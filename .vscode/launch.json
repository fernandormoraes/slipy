{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "slipy version",
            "request": "launch",
            "program": "../bin/slipy.dart",
            "cwd": "example",
            "type": "dart",
            "args": ["--version"]
        },
        {
            "name": "slipy install",
            "request": "launch",
            "program": "../bin/main.dart",
            "cwd": "example",
            "type": "dart",
            "args": ["install", "triple"]
        },
        {
            "name": "slipy install error",
            "request": "launch",
            "program": "../bin/main.dart",
            "cwd": "example",
            "type": "dart",
            "args": ["install", "tripple"]
        },
        {
            "name": "slipy uninstall",
            "request": "launch",
            "program": "../bin/slipy.dart",
            "cwd": "example",
            "type": "dart",
            "args": ["uninstall", "triple"]
        },
        {
            "name": "slipy start",
            "request": "launch",
            "program": "../bin/slipy.dart",
            "cwd": "example",
            "console": "terminal",
            "type": "dart",
            "args": ["start", "-f"]
        },
        {
            "name": "slipy generate m 1",
            "request": "launch",
            "program": "../bin/slipy.dart",
            "cwd": "example",
            "type": "dart",
            "args": ["g", "m","home"]
        },
        {
            "name": "slipy generate m 2",
            "request": "launch",
            "program": "../bin/slipy.dart",
            "cwd": "example",
            "type": "dart",
            "args": ["g", "m","home/internal"]
        },{
            "name": "slipy generate triple",
            "request": "launch",
            "program": "../bin/slipy.dart",
            "cwd": "example",
            "type": "dart",
            "args": ["g", "triple","home/home"]
        
        },{
            "name": "slipy run reload",
            "request": "launch",
            "program": "bin/main.dart",
            "type": "dart",
            "args": ["run", "reload"]
        
        },{
            "name": "slipy run complete",
            "request": "launch",
            "program": "bin/main.dart",
            "type": "dart",
            "args": ["run", "complete"]
        }
    ]
}